#include <stdio.h>
int main()
{
    int digit,n;
    int even=0,odd=0,zero=0;
    scanf("%d",&n);
    if(n==0)
    zero==1;
while(n>0)
{
	digit=n%10;
	if(digit==0){
		zero++;
	}
	else if(digit%2==0)
	{
		even++;
	}
	else
	{
		odd++;
	}
	n=n/10;
}
printf("zero %d\n",zero);
printf("even %d\n",even);
printf("odd %d\n",odd);
return 0;
}
